"tree_ID" = c(as.integer(my.tree.id)),
"inv" = c(my.inv),
"lon" = c(as.numeric(tree.coord.df$lon)),
"lat" = c(as.numeric(tree.coord.df$lat)),
"t_stat" = c(tree_status))
)
